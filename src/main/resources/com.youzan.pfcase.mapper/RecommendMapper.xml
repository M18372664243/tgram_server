<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.youzan.pfcase.mapper.RecommendMapper">

    <cache />

    <select id="getAllRecommend" parameterType="string" resultType="Recommend">
        SELECT
        *
        from recommend where enabled=1 order by seq asc
    </select>
    
</mapper>

